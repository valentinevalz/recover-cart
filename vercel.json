{
  "version": 2,
  "framework": "nextjs",
  "builds": [
    {
      "src": "recover-cart/package.json",
      "use": "@vercel/next"
    }
  ],
  "functions": {
    "recover-cart/app/api/**/*.js": {
      "runtime": "nodejs20.x"
    }
  },
  "ignoreCommand": "git check-ignore **/*",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "recover-cart/$1"
    }
  ]
}